    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome index",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://127.0.0.1:8090/pages/index.html",
            "webRoot": "${workspaceFolder}"
        },

        {
            "type": "pwa-node",
            "request": "launch",
            "name": "js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\js\\index.js"
        }
    ]
}